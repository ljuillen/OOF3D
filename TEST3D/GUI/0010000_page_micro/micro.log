# OOF version (unreleased)
OOF.Microstructure.New(name='micro1', width=5, height=5, depth=1.0, width_in_pixels=100, height_in_pixels=100, depth_in_pixels=10)
OOF.PixelGroup.New(name='abcde', microstructure='micro1')
OOF.PixelGroup.New(name='fghij', microstructure='micro1')
OOF.PixelGroup.Rename(microstructure='micro1', group='abcde', new_name='klmno')
OOF.PixelGroup.Meshable(microstructure='micro1', group='klmno', meshable=False)
OOF.PixelGroup.Delete(microstructure='micro1', group='klmno')
OOF.PixelSelection.Invert(microstructure='micro1')
OOF.PixelGroup.AddSelection(microstructure='micro1', group='fghij')
OOF.PixelGroup.Clear(microstructure='micro1', group='fghij')
OOF.File.Save.Microstructure(filename='micro.dat', mode='w', format='ascii', microstructure='micro1')
