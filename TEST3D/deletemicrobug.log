# After loading this script, OOF3D hangs when quitting.
# Commenting out one of the Scheduled_Output definitions fixes the problem.
# OOF version (unreleased)
OOF.Microstructure.New(name='microstructure', width=1.0, height=1.0, depth=1.0, width_in_pixels=10, height_in_pixels=10, depth_in_pixels=10)
OOF.Property.Copy(property='Mechanical:Elasticity:Isotropic', new_name='instance')
OOF.Material.New(name='material', material_type='bulk')
OOF.Material.Add_property(name='material', property='Mechanical:Elasticity:Isotropic:instance')
OOF.Property.Copy(property='Mechanical:MassDensity:ConstantMassDensity', new_name='instance')
OOF.Material.Add_property(name='material', property='Mechanical:MassDensity:ConstantMassDensity:instance')
OOF.Material.Add_property(name='material', property='Thermal:Conductivity:Isotropic')
OOF.Material.Assign(material='material', microstructure='microstructure', pixels=every)
OOF.Skeleton.New(name='skeleton', microstructure='microstructure', x_elements=4, y_elements=4, z_elements=4, skeleton_geometry=TetraSkeleton(arrangement='moderate'))
OOF.Mesh.New(name='mesh', skeleton='microstructure:skeleton', element_types=['TET4_4', 'Q4_4', 'T3_3', 'D2_2'])
OOF.Subproblem.Field.Define(subproblem='microstructure:skeleton:mesh:default', field=Displacement)
OOF.Subproblem.Field.Activate(subproblem='microstructure:skeleton:mesh:default', field=Displacement)
OOF.Subproblem.Equation.Activate(subproblem='microstructure:skeleton:mesh:default', equation=Force_Balance)
OOF.Mesh.Scheduled_Output.New(mesh='microstructure:skeleton:mesh', name=AutomaticName('Displacement//Direct Output'), output=ScheduledAnalysis(data=getOutput('Field:Value',field=Displacement),operation=DirectOutput(),domain=EntireMesh(),sampling=GridSampleSet(x_points=10,y_points=10,z_points=10,show_x=True,show_y=True,show_z=True)), scheduletype=AbsoluteOutputSchedule(), schedule=Periodic(delay=0.0,interval=0.0), destination=OutputStream(filename='/Users/langer/FE/OOF3D/TEST3D/abcde',mode='w'))
OOF.Mesh.Scheduled_Output.New(mesh='microstructure:skeleton:mesh', name=AutomaticName('Temperature//Direct Output'), output=ScheduledAnalysis(data=getOutput('Field:Value',field=Temperature),operation=DirectOutput(),domain=EntireMesh(),sampling=GridSampleSet(x_points=10,y_points=10,z_points=10,show_x=True,show_y=True,show_z=True)), scheduletype=AbsoluteOutputSchedule(), schedule=Periodic(delay=0.0,interval=0.0), destination=OutputStream(filename='/Users/langer/FE/OOF3D/TEST3D/abcdef',mode='w'))
OOF.Mesh.Scheduled_Output.New(mesh='microstructure:skeleton:mesh', name=AutomaticName('GraphicsUpdate'), output=GraphicsUpdate(), scheduletype=AbsoluteOutputSchedule(), schedule=Periodic(delay=0.0,interval=1), destination=GfxWindowDestination())
#OOF.Mesh.Scheduled_Output.New(mesh='microstructure:skeleton:mesh', name=AutomaticName('GraphicsUpdate'), output=GraphicsUpdate(), scheduletype=AbsoluteOutputSchedule(), schedule=Periodic(delay=0.0,interval=1), destination=GfxWindowDestination())
OOF.Microstructure.Delete(microstructure='microstructure')
